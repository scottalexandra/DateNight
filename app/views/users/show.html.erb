<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=La+Belle+Aurore" />
<div class="show container profile-info", id="full-length-background" style = "margin-left: 0px, margin-right: 0px; width: 100%; height: 1000px;">
  <h1><%= current_user.name %>'s Profile</h1>
  <div class="panel panel-default upcoming-dates">
    <div class="panel-heading">My Date Itineraries</div>
    <div class="panel-body">
      <% @itineraries.each do |itinerary| %>
      <div class="itinerary-display">
        <%= link_to itinerary.title, itinerary_path(itinerary.id) %><br>
        <%= itinerary.description %>
        <span class="destroy-itinerary">
          <%= link_to "delete", itinerary_path(itinerary.id), method: :delete %>
        </span><hr>
      </div>
      <% end %>
    </div>
  </div>
</div>
