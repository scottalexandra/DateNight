<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=La+Belle+Aurore" />
<!-- <div class="show-container"> -->
  <!-- <div class="user-profile-heading"> -->
  <!-- </div> -->
  <div class="show container profile-info", style = "margin-left: 0px, margin-right: 0px; width: 100%">
    <h1><%= @user.name%> > My Profile</h1>
    <div class="panel panel-default upcoming-dates">
      <div class="panel-heading">Upcoming Dates</div>
      <div class="panel-body">No Upcoming Dates</div>
    </div>
    <div class="panel panel-default past-dates">
      <div class="panel-heading">Past Dates</div>
      <div class="panel-body">No Past Dates</div>
    </div>
    <div class="panel panel-default account-details">
      <div class="panel-heading">Account Details</div>
      <div class="panel-body">
        <div class="name">
          name: <%= @user.name %>
          <%= link_to "edit", edit_user_path(@user.id), class: "edit" %>
        </div><br>
        <div class="email">
          email: <%= @user.email %>
          <%= link_to "edit", edit_user_path(@user.id), class: "edit" %>
        </div>
      </div>
    </div>
  </div>
<!-- </div> -->
